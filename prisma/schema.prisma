datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Usuario {
  id        String  @id @default(uuid())
  nome      String
  email     String  @unique
  senhaHash String
  role      String  @default("viewer")
  oculto    Boolean @default(false)
}
